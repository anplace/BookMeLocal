{
  "name": "bookmelocal",
  "version": "1.0.0",
  "description": "A website that allows users to book appointments with local businesses. The business, like a car mechanic, can tie their scheduler into this app, then users can select time slots. The user will then have a master calendar that has all of their appointments listed on it.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "watch": "nodemon server.js",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:booking": "npx sequelize-cli db:seed --seed 07-create-Booking.js"
  },
  "keywords": [],
  "author": "Luis Llamas",
  "license": "ISC",
  "dependencies": {
    "@fullcalendar/core": "^6.1.11",
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/interaction": "^6.1.11",
    "@fullcalendar/list": "^6.1.11",
    "@fullcalendar/multimonth": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.11",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^7.1.7",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "fullcalendar": "^6.1.11",
    "fullcalendar-scheduler": "^6.1.11",
    "helmet": "^7.1.0",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "mysql2": "^3.9.1",
    "parse-database-url": "^0.3.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.37.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "nodemon": "^3.1.0"
  }
}
